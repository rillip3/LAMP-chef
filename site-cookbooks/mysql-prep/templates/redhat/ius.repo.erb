[ius]
name=IUS Community Packages for Enterprise Linux <%= @version %> - $basearch
baseurl=http://mirror.rackspace.com/ius/stable/Redhat/<%= @version %>/$basearch
        http://dl.iuscommunity.org/pub/ius/stable/Redhat/<%= @version %>/$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/IUS-COMMUNITY-GPG-KEY

[ius-debuginfo]
name=IUS Community Packages for Enterprise Linux <%= @version %> - $basearch - Debug
baseurl=http://mirror.rackspace.com/ius/stable/Redhat/<%= @version %>/$basearch/debuginfo
        http://dl.iuscommunity.org/pub/ius/stable/Redhat/<%= @version %>/$basearch/debuginfo
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/IUS-COMMUNITY-GPG-KEY
gpgcheck=1

[ius-source]
name=IUS Community Packages for Enterprise Linux <%= @version %> - $basearch - Source
baseurl=http://mirror.rackspace.com/ius/stable/Redhat/<%= @version %>/SRPMS
        http://dl.iuscommunity.org/pub/ius/stable/Redhat/<%= @version %>/SRPMS
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/IUS-COMMUNITY-GPG-KEY
gpgcheck=1
